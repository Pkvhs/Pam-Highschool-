  .custom-modal .modal-content .modal-body[data-id="false"] { height: auto; min-height: 5rem; }
  .custom-modal .modal-content .modal-body { padding: 0px; }
}

.custom-modal .modal-content .modal-footer { display: flex; justify-content: flex-end; }

@media only screen and (max-width: 768px) {
  .custom-modal .modal-content .modal-footer { padding-top: 1rem; }
}

.custom-modal .modal-content .modal-footer button[type="submit"] { margin-left: 2rem; }

.edit-btn { background-color: initial; display: grid; font-size: 2.5rem; font-weight: 500; padding: 0.4em 0.5em; place-items: center; }

.edit-btn a { padding: 0px !important; }

.edit-btn, .edit-btn:hover { color: var(--dark-black); }

.edit-btn:hover { cursor: pointer; }

.wrapper-class { font-size: 10px; font-weight: 500; }

.wrapper-class .rich-text-editor { font-size: 1.6em; font-weight: inherit; }

.wrapper-class .rich-text-editor h1, .wrapper-class .rich-text-editor h2, .wrapper-class .rich-text-editor h3 { color: inherit; font-weight: 700; }

.wrapper-class .rich-text-editor h1 { font-weight: 700; font-size: 3.2em !important; }

.wrapper-class .rich-text-editor h2 { font-size: 2.4em !important; }

.wrapper-class .rich-text-editor h3 { font-size: 1.872em !important; }

.modal__wrapper { height: 100%; left: 100%; opacity: 0; position: fixed; top: 0px; transition: opacity 0.3s ease-in-out 0s, left, top; width: 100%; z-index: 99999; }

.modal__content { background-color: rgb(255, 255, 255); box-shadow: rgba(0, 0, 0, 0.3) 0px -5px 10px; margin-left: auto; min-height: 100%; position: absolute; right: 0px; top: 0px; width: 30%; }

@media only screen and (max-width: 576px) {
  .modal__content { width: 100%; }
}

.modal__show { left: 0px; opacity: 1; overflow: auto; transition: opacity 0.3s ease-in-out 0s, left; }

.modal__bg { background-color: rgba(0, 0, 0, 0.5); transition: all 0.3s ease 0s; }

.modal__close-button { background-color: var(--dark-black); border-radius: 5px; color: rgb(255, 255, 255); font-size: 2em; left: 40px; padding: 0px 0.5em 0.2em; position: absolute; top: 20px; }

@media only screen and (max-width: 768px) {
  .modal__close-button { left: 40px; top: 20px; }
}

.modal__footer-label { background-color: var(--dark-black); border-radius: 0px 0px 10px 10px; bottom: -0.5em; color: rgb(255, 255, 255); left: 0px; padding: 0.5em; position: absolute; text-align: center; }

@media only screen and (max-width: 576px) {
  .modal__footer-label { bottom: -2em; }
}

.modal__wrapper#confirm-service { left: 0px; }

.modal__show#confirm-service { animation: 0.3s cubic-bezier(0.175, 0.885, 0.32, 1.275) 0s 1 normal none running append-animate; }

@-webkit-keyframes append-animate { 
  0% { left: 0px; opacity: 0; top: 100%; }
  100% { left: 0px; opacity: 1; top: 0px; }
}

@keyframes append-animate { 
  0% { left: 0px; opacity: 0; top: 100%; }
  100% { left: 0px; opacity: 1; top: 0px; }
}

